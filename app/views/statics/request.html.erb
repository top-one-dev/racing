<%= simple_form_for(:request, :method => :get) do |f| %>
  <div class="form-inputs">
    <%= f.input :request_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, value: "Send Request" %>
  </div>
<% end %>
<hr>
<div>
<%= session[:access_token].to_s %>
<%= @url %>
<hr>
	<%= @result.to_s unless @result.nil? %>
</div>